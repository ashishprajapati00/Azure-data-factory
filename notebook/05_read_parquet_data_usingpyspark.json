					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"dfnullfilter= dfselect.filter(col(\"Resourcegroup\").isNull())\r\n",
					"display(dfnullfilter)"
				],
				"execution_count": null
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
