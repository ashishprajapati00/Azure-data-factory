							"deleting": false
						}
					}
				},
				"source": [
					"from pyspark.sql.functions import col\r\n",
					"\r\n",
					"\r\n",
					"dfselect=df.select(col(\"CorrelationId\"),col(\"OperationName\"),col(\"Resourcegroup\"))\r\n",
					"display(dfselect)\r\n",
					""
