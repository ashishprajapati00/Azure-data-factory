{
	"name": "08_dataflow_readweeblog",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "stagingwebloginputmetric201",
						"type": "DatasetReference"
					},
					"name": "stagingweblogInputStream"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "stagingweblogoutput",
						"type": "DatasetReference"
					},
					"name": "stagingweblogoutput",
					"description": "Export data to "
				}
			],
			"transformations": [
				{
					"name": "select"
				}
			],
			"scriptLines": [
				"source(output(",
				"          count as integer,",
				"          total as integer,",
				"          minimum as integer,",
				"          maximum as integer,",
				"          resourceId as string,",
				"          time as string,",
				"          metricName as string,",
				"          timeGrain as string,",
				"          AccountResourceId as string,",
				"          ApiName as string,",
				"          Authentication as string,",
				"          GeoType as string,",
				"          ResponseType as string,",
				"          TransactionType as string,",
				"          average as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     documentForm: 'documentPerLine') ~> stagingweblogInputStream",
				"stagingweblogInputStream select(mapColumn(",
				"          time,",
				"          metricName,",
				"          average",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select",
				"select sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> stagingweblogoutput"
			]
		}
	}
}